{
  "buildOptions": {
    "debugType": "portable",
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "InternalsVisibleTo.fs",
        "../../assemblyinfo/assemblyinfo.FSharp.Build.dll.fs",
        "obj/FSBuild.fs",
        "../../utils/CompilerLocationUtils.fs",
        "../../utils/reshapedreflection.fs",
        "Fsc.fsi",
        "Fsc.fs",
        "CreateFSharpManifestResourceName.fsi",
        "CreateFSharpManifestResourceName.fs"
      ]
    }
  },
  "define": [
    "NETSTANDARD1_6",
    "FX_RESHAPED_REFLECTION",
    "FX_ATLEAST_35",
    "NO_WIN_REGISTRY",
    "FX_NO_APP_DOMAINS",
    "FX_NO_SYSTEM_CONFIGURATION",
    "FX_ATLEAST_45",
    "SIGNED",
    "STRONG_NAME_AND_DELAY_SIGN_FSHARP_COMPILER_WITH_MSFT_KEY"
  ],
  "embed": {
    "mappings": {
      "FSBuild": "FSBuild.resx"
    }
  },
  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*",
    "Microsoft.Build": "0.1.0-preview-00028-160627",
    "Microsoft.Build.Framework": "0.1.0-preview-00028-160627",
    "Microsoft.Build.Tasks.Core": "0.1.0-preview-00028-160627",
    "Microsoft.Build.Utilities.Core": "0.1.0-preview-00028-160627",
    "Microsoft.NETCore.Platforms": "1.0.1",
    "Microsoft.Win32.Registry": {
      "version": "4.0.0",
      "exclude": "Compile"
    },
    "System.AppContext": "4.1.0",
    "System.Diagnostics.Tools": "4.0.1",
    "System.Reflection.Primitives": "4.0.1",
    "System.Resources.ResourceManager": "4.0.1"
  },
  "tools": {
    "dotnet-fssrgen": "3.2.*",
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": "dnxcore50"
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "portable-net45+win8"
    }
  }
